<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background-->
    <Color x:Key="primaryBackColor1">#262626</Color>
    <Color x:Key="primaryBackColor2">#1c1c1c</Color>
    <Color x:Key="secondaryBackColor1">#0d0d0d</Color>
    <Color x:Key="secondaryBackColor2">#1f1f1f</Color>
    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#313131</Color>
    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#666666"/>
    <SolidColorBrush x:Key="color2" Color="#666666"/>
    <SolidColorBrush x:Key="color3" Color="#666666"/>
    <SolidColorBrush x:Key="color4" Color="#6e1414"/>
    <SolidColorBrush x:Key="color5" Color="#7E82FC"/>
    <SolidColorBrush x:Key="color6" Color="#FFC047"/>
    <SolidColorBrush x:Key="color7" Color="#EF6C96"/>
    <SolidColorBrush x:Key="color8" Color="#152369"/>
    <SolidColorBrush x:Key="color9" Color="#07F3C0"/>
    <SolidColorBrush x:Key="color10" Color="#FBA1AA"/>
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="titleColor2" Color="#a6a6a6"/>
    <SolidColorBrush x:Key="titleColor3" Color="#a6a6a6"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#666666"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#200F53"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#1a1a1a"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#363636"/>
</ResourceDictionary>