<UserControl x:Class="Booking.Views.Scenes.HomeScene"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:views="clr-namespace:Booking.Views"
             xmlns:local="clr-namespace:Booking.Views.Scenes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Command="{Binding UserAdminWindowCommand}"  Width="60" Height="30">Admin</Button>
        <TextBlock Grid.Column="1" FontSize="20" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Home View"></TextBlock>
    </Grid>
</UserControl>
